    <script type="text/javascript">
        $(function($) {
                $(".nav-inner>.xwzx").addClass('current').siblings().removeClass('current');

            }
        );

        $("#tuijian").click(function(){
            $("#tuijian").attr("href","mailto:?body=静安门户网站提醒!您的好友为您推荐:"+window.location.href+"");
        });
        $(".detailmessage a").click(function(){
            //获取para的字体大小
            var thisEle;
            if($(".infoContent_info span").css("font-size")!=null){
                thisEle= $(".infoContent_info span").css("font-size");
            }else{
                thisEle= $(".infoContent_info p,.infoContent_info,.infoContent_info font,.infoContent_info span,.infoContent_info div").css("font-size");
            }

            //parseFloat的第二个参数表示转化的进制，10就表示转为10进制
            var textFontSize = parseFloat(thisEle , 10);
            //javascript自带方法
            var unit = thisEle.slice(-2); //获取单位
            var cName = $(this).attr("class");
            if(cName == "bigger"){
                if( textFontSize <= 22 ){
                    textFontSize += 2;
                }
            }else if(cName == "smaller"){
                if( textFontSize >= 6 ){
                    textFontSize -= 2;
                }
            }
            //设置para的字体大小
            $(".infoContent_info p,.infoContent_info,.infoContent_info font,.infoContent_info span,.infoContent_info div").css("font-size",  textFontSize + unit );
        });
        $(".medium").click(function(){
            $(".infoContent_info p,.infoContent_info,.infoContent_info font,.infoContent_info span,.infoContent_info div").css("font-size","14px");
        })

        $(".infoContent_info a").each(function(){
            var url=$(this).attr('href');
            if(url.indexOf("NewsUpload")>=0){
                url = url.split("/");
                url.pop();
                url =url.join("/");
                $(this).attr("href",url);

            }

            //url就是遍历出来的单个a标签网址，txt就是a之间的文本。
        });
        $("#divInfoReportPage").attr("style", "margin: 0 auto");
        function EnterPress(e){
            var e = e || window.event;
            if(e.keyCode == 13){
                var audio = document.getElementById("bgMusic");
                audio.play();
            }
        }
    </script>
